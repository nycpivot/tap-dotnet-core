@using System.Collections

<div class="text-center">
    <h1 class="display-4">Environment</h1>

    <p>@Html.ValidationMessage("Export")</p>

    <table>
        @foreach (DictionaryEntry variable in ViewBag.Variables)
        {
            <tr>
                <td>@variable.Key</td>
                <td>@variable.Value</td>
            </tr>
        }
    </table>
</div>